	<queryString language="SQL">
		<![CDATA[IF $P{actualYear} = ''
        BEGIN
            SELECT
                 VProjectList."RowNr" AS VProjectList_RowNr,
                 VProjectList."projectID" AS VProjectList_projectID,
                 VProjectList."projectName" AS VProjectList_projectName,
                 VProjectList."projectTypeName" AS VProjectList_projectTypeName,
                 VProjectList."employeeName" AS VProjectList_employeeName,
                 VProjectList."customerName" AS VProjectList_customerName,
                 VProjectList."actualMM" AS VProjectList_actualMM,
                 VProjectList."actualStartDate" AS VProjectList_actualStartDate,
                 VProjectList."actualEndDate" AS VProjectList_actualEndDate,
                 VProjectList."plannedStartDate" AS VProjectList_plannedStartDate,
                 VProjectList."plannedEndDate" AS VProjectList_plannedEndDate
            FROM "dbo"."VProjectList" VProjectList
            WHERE   VProjectList."projectTypeName" LIKE N'%'+ $P{projectTypeName} + '%'
                    AND VProjectList."customerName" LIKE N'%'+ $P{customerName} + '%'
                    AND VProjectList."employeeName" LIKE N'%'+ $P{employeeName} + '%'

         END
         ELSE
            SELECT
                 VProjectList."RowNr" AS VProjectList_RowNr,
                 VProjectList."projectID" AS VProjectList_projectID,
                 VProjectList."projectName" AS VProjectList_projectName,
                 VProjectList."projectTypeName" AS VProjectList_projectTypeName,
                 VProjectList."employeeName" AS VProjectList_employeeName,
                 VProjectList."customerName" AS VProjectList_customerName,
                 VProjectList."actualMM" AS VProjectList_actualMM,
                 VProjectList."actualStartDate" AS VProjectList_actualStartDate,
                 VProjectList."actualEndDate" AS VProjectList_actualEndDate,
                 VProjectList."plannedStartDate" AS VProjectList_plannedStartDate,
                 VProjectList."plannedEndDate" AS VProjectList_plannedEndDate
            FROM "dbo"."VProjectList" VProjectList
            WHERE   VProjectList."projectTypeName" LIKE N'%'+ $P{projectTypeName} + '%'
                    AND VProjectList."customerName" LIKE N'%'+ $P{customerName} + '%'
                    AND VProjectList."employeeName" LIKE N'%'+ $P{employeeName} + '%'
                    AND Year(VProjectList."actualStartDate") LIKE '%'+ $P{actualYear}+ '%']]>
	</queryString>
