<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>

<noscript>
	<meta http-equiv="refresh"
		content="0;url=<c:url value='/invalids/no-script.html'/>" />
</noscript>
<!--[if lt IE 8]>
	<meta http-equiv="refresh" content="0;url=<c:url value='/invalids/ie-not-support.html'/>" />
<![endif]-->

<script type="text/javascript">
	// Detect firefox version

	var userAgent = navigator.userAgent;

	if (/Firefox[\/\s](\d+\.\d+)/.test(userAgent)) { // Firefox
		var ffversion = new Number(RegExp.$1) // 
		if (ffversion < 8) {
			window.location = "<c:url value='/invalids/firefox-not-support.html'/>";
		}
	} else if ((userAgent.toLowerCase().indexOf('chrome') > -1)) { // Chrome
		var indexOfChrome = Number(userAgent.toLowerCase().indexOf('chrome')) + 7;
		var ver = Number(userAgent.substring(indexOfChrome, indexOfChrome + 4));
		if (ver < 15) {
			window.location = "<c:url value='/invalids/chrome-not-support.html'/>";
		}
	} else if (/MSIE (\d+\.\d+);/.test(userAgent)) { // IE

	} else if (userAgent.toLowerCase().lastIndexOf('safari/') > 0) { // Safari
		var ver = Number(userAgent.substr(userAgent.toLowerCase().lastIndexOf(
				'safari/') + 7, 5));
		if (ver < 500) {
			window.location = "<c:url value='/invalids/safari-not-support.html'/>";
		}
	} else {
		window.location = "<c:url value='/invalids/browser-not-support.html'/>";
	}
</script>