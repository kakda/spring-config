<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mobilecnc.security.service.SecurityDAO">
	<resultMap id="permission" type="Permission">
		<result property="entity" column="entity"/>
		<result property="add" column="Add"/>
        <result property="addCode" column="AddCode"/>
        <result property="delete" column="Delete"/>
        <result property="deleteCode" column="DeleteCode"/>
		<result property="update" column="Update"/>
		<result property="updateCode" column="UpdateCode"/>
		<result property="view" column="View"/>
        <result property="viewCode" column="ViewCode"/>
        <result property="entry" column="entry"/>
        <result property="entryCode" column="EntryCode"/>
        <result property="unsubmit" column="unsubmit"/>
        <result property="unsubmitCode" column="UnsubmitCode"/>
        <result property="accountCode" column="AccountCode"/>
        <result property="enableAccount" column="Enable_Account"/>
        <result property="resetCode" column="ResetCode"/>
        <result property="resetPassword" column="Reset_Password"/>
        <result property="reportCode" column="ReportCode"/>
        <result property="report" column="Report"/>
        <result property="all" column="All"/>
    </resultMap>
	<select id="getPermissions" parameterType="String" statementType="CALLABLE" resultMap="permission">
    	{ CALL getPermissions(#{roleID})}
	</select>
</mapper>